local function PopUp(Text,Bad)
	if settingsss["Show PopUp"] == true then
		task.spawn(function()
			local PopUp_2 = Instance.new("Frame")
			local UICorner = Instance.new("UICorner")
			local Icon = Instance.new("ImageLabel")
			local PlaySound = Instance.new("Sound")
			local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
			local Title = Instance.new("TextLabel")
			PlaySound.SoundId = "rbxassetid://17582299860"
			PlaySound.Parent = spw
			PopUp_2.Name = "PopUp"
			PopUp_2.Parent = Here
			PopUp_2.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
			PopUp_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
			PopUp_2.BorderSizePixel = 0
			PopUp_2.Size = UDim2.new(0, 0, 0, 0)
			if settingsss["Animations"] == true then
				ts:Create(PopUp_2,TweenInfo.new(0.4,Enum.EasingStyle.Sine,Enum.EasingDirection.Out),{Size = UDim2.new(1, 0, 0, 60)}):Play()
			end
			UICorner.CornerRadius = UDim.new(0.200000003, 0)
			UICorner.Parent = PopUp_2
			Icon.Name = "Icon"
			Icon.Parent = PopUp_2
			Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Icon.BackgroundTransparency = 1.000
			Icon.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Icon.BorderSizePixel = 0
			Icon.Position = UDim2.new(0.0538020171, 0, 0.0833333358, 0)
			Icon.Size = UDim2.new(0.182496399, 0, 0.833333313, 0)
			Icon.Image = "rbxassetid://18460146845"
			UIAspectRatioConstraint.Parent = Icon
			Title.Name = "Title"
			Title.Parent = PopUp_2
			Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Title.BackgroundTransparency = 1.000
			Title.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Title.BorderSizePixel = 0
			Title.Position = UDim2.new(0.254662454, 0, 0, 0)
			Title.Size = UDim2.new(0.745336711, 0, 1, 0)
			Title.Font = Enum.Font.Roboto
			Title.TextColor3 = Color3.fromRGB(255,255,255)
			Title.Text = tostring(Text)
			Title.TextScaled = true
			Title.TextSize = 14.000
			Title.TextWrapped = true
			Title.TextXAlignment = Enum.TextXAlignment.Left
			if Bad == true then
				ErrorSSound()
				Title.TextColor3 = Color3.new(1,0,0)
			else
				PlaySound:Play()
			end
			task.wait(4)
			if settingsss["Animations"] == true then
				ts:Create(PopUp_2,TweenInfo.new(0.4,Enum.EasingStyle.Sine,Enum.EasingDirection.Out),{Size = UDim2.new(0, 0, 0, 60)}):Play()
			end
			task.wait(1)
			PlaySound:Destroy()
			PopUp_2:Destroy()
		end)
	end
end
if versions == pluginvers then
else
	PopUp("Your plugin version is outdated please use new version.")
end
local function CreateCacheFolder()
	local Checking = game.ServerStorage:FindFirstChild("81396126331815_AntiVirus_Cache")
	local Checker = game.ServerStorage:FindFirstChild("81396126331815_AntiVirus_Quarantine")
	if not Checking then
		local NewCacheFolder = Instance.new("Folder")
		NewCacheFolder.Name = "81396126331815_AntiVirus_Cache"
		NewCacheFolder.Parent = game.ServerStorage
	end
	if not Checker then
		local NewQuarantFold = Instance.new("Folder")
		NewQuarantFold.Name = "81396126331815_AntiVirus_Quarantine"
		NewQuarantFold.Parent = game.ServerStorage
	end
end
local function CreateInfraction(txt,path)
	if not game.ServerStorage:FindFirstChild("81396126331815_AntiVirus_Cache") then
		CreateCacheFolder()
	end
	if not game.ServerStorage["81396126331815_AntiVirus_Cache"]:FindFirstChild(path) then
		local NewInfraction = Instance.new("StringValue")
		NewInfraction.Name = path
		NewInfraction.Value = txt
		NewInfraction.Parent = game.ServerStorage["81396126331815_AntiVirus_Cache"]
	end
end
local function CreateReadMeScript(source,parent)
	local ouhohuhou,uouo = pcall(function()
		if parent:FindFirstChild("Anti_Virus_ReadMe") then
			parent:FindFirstChild("Anti_Virus_ReadMe"):Destroy()
			local NewRMSc = Instance.new("ModuleScript",parent)
			NewRMSc.Name = "Anti_Virus_ReadMe"
			NewRMSc.Source = "--[[\n"..logo.."\n]]\n".."--[[\n"..source.."\n]]\n\n--[[\n"..warning_logo..']]'
		else
			local NewRMSc = Instance.new("ModuleScript",parent)
			NewRMSc.Name = "Anti_Virus_ReadMe"
			NewRMSc.Source = "--[[\n"..logo.."\n]]\n".."--[[\n"..source.."\n]]\n\n--[[\n"..warning_logo.."]]"
		end
	end)
	if ouhohuhou then
	else
		warn("Need permissions")
		PopUp("You need to give plugin permissions to create readme script")
	end
end
local function CreateError(Script,pls)
	if not Script or not Script:IsDescendantOf(game) then
		return
	end
	task.spawn(function()
		local NewFrame = Instance.new("Frame")
		NewFrame.Size = UDim2.new(1,0,0,20)
		NewFrame.Position = UDim2.fromScale(0,0)
		NewFrame.BorderSizePixel = 0
		NewFrame.BackgroundTransparency = 1
		NewFrame.Parent = pls
		local NewText = Instance.new("TextLabel")
		NewText.Size = UDim2.new(0.9,0,1,0)
		NewText.Position = UDim2.fromScale(0,0)
		NewText.BorderSizePixel = 0
		NewText.TextScaled = true
		NewText.BackgroundTransparency = 1
		NewText.Font = Enum.Font.Code
		NewText.TextColor3 = Color3.fromRGB(0,255,0)
		NewText.TextXAlignment = Enum.TextXAlignment.Left
		NewText.Text = "Virus found : | (name) : " .. Script.Name .. " | Parent : " .. Script:GetFullName() .. " | Attempt to do bad stuff (script disabled)"
		CreateInfraction(NewText.Text,Script:GetFullName())
		if settingsss["Disable suspected scripts"] == true then
			if Script.ClassName == "ModuleScript" then
				CreateCacheFolder()
				local yesouno,responce = pcall(function()
					if Script.Parent == game.ServerStorage:FindFirstChild("81396126331815_AntiVirus_Quarantine") and Script.Source:find("--Quarantined by AntiVirus Plugin. Content has been neutralized.") then
					else
						local OldPath = Script:GetFullName()
						Script.Parent = game.ServerStorage["81396126331815_AntiVirus_Quarantine"]
						local lines = Script.Source:split("\n")
						for i, line in ipairs(lines) do
							lines[i] = "-- " .. line
							task.wait()
						end
						Script.Source = "--Quarantined by AntiVirus Plugin. Content has been neutralized.\n--Everything below this block is part of the module.\n--This block will always be first when script is quarantined.\n\n" .. table.concat(lines, "\n")
						CreateReadMeScript("This script used to be in here : "..OldPath.. " | The last name after the last . is the script location\nThis script was considered suspect and you can disable this feature if you don't want this to happen again",Script)
					end
				end)
				if yesouno then
				else
					warn("Need permission or something else")
					PopUp("You need to give plugin permissions to put scripts to quarantine.",true)
				end
			else
				Script.Disabled = true
				if settingsss["Quarantine scripts (places script where it can't be executed)"] == true then
					CreateCacheFolder()
					local yesouno,responce = pcall(function()
						if Script.Parent == game.ServerStorage:FindFirstChild("81396126331815_AntiVirus_Quarantine") and Script.Source:find("--Quarantined by AntiVirus Plugin. Content has been neutralized.") then
						else
							local OldPath = Script:GetFullName()
							Script.Parent = game.ServerStorage["81396126331815_AntiVirus_Quarantine"]
							local lines = Script.Source:split("\n")
							for i, line in ipairs(lines) do
								lines[i] = "-- " .. line
								task.wait()
							end
							Script.Source = "--Quarantined by AntiVirus Plugin. Content has been neutralized.\n--Everything below this block is part of the module.\n--This block will always be first when script is quarantined.\n\n" .. table.concat(lines, "\n")
							CreateReadMeScript("This script used to be in here : "..OldPath.. " | The last name after the last . is the script location\nThis script was considered suspect and you can disable this feature if you don't want this to happen again",Script)
						end
					end)
					if yesouno then
					else
						warn("Need permission or something else")
						PopUp("You need to give plugin permissions to put scripts to quarantine.",true)
					end
				end
			end
		end
		PopUp("New virus found in your game :"..Script:GetFullName(),true)
		NewText.Parent = NewFrame
		local NewButton = Instance.new("TextButton")
		NewButton.Size = UDim2.new(0.1,0,0.8,0)
		NewButton.Position = UDim2.new(0.9,0,0.1,0)
		NewButton.BorderSizePixel = 0
		NewButton.TextScaled = true
		NewButton.BackgroundColor3 = Color3.fromRGB(255,0,0)
		NewButton.Text = "Open Script"
		NewButton.Font = Enum.Font.Code
		NewButton.TextColor3 = Color3.fromRGB(255,255,255)
		NewButton.Parent = NewFrame
		Instance.new("UICorner",NewButton)
		NewButton.MouseButton1Click:Connect(function()
			if Script and Script:IsDescendantOf(game) then
				plugin:OpenScript(Script, 1)
			else
				PopUp("Impossible of opening the script because it got deleted.",true)
			end
		end)
	end)
end
