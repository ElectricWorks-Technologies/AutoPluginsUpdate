local function colorLuaTableString(str)
	str = str:gsub('(")(.-)(")', function(open, content, close)
		content = content
			:gsub("&", "&amp;")
			:gsub("<", "&lt;")
			:gsub(">", "&gt;")

		local openColor = '<font color="rgb(255,255,255)">' .. open .. '</font>'
		local contentColor = '<font color="rgb(0,255,0)">' .. content .. '</font>'
		local closeColor = '<font color="rgb(255,255,255)">' .. close .. '</font>'
		return openColor .. contentColor .. closeColor
	end)
	str = str:gsub("([^%w_%.])(%-?%d+%.?%d*)", function(before, number)
		return before .. `<font color="rgb(255,255,0)">` .. number .. `</font>`
	end)
	str = str:gsub("(%f[%a])(true)(%f[%A])", '<font color="rgb(255,255,0)">%2</font>')
	str = str:gsub("(%f[%a])(false)(%f[%A])", '<font color="rgb(255,255,0)">%2</font>')
	str = str:gsub("(%f[%a])(nil)(%f[%A])", '<font color="rgb(255,255,0)">%2</font>')
	return str
end
function stt(str)
	local sus,module = pcall(function()
		local module = Instance.new("ModuleScript")
		module.Source = "return " .. str
		return module
	end)
	if sus then
		local success, result = pcall(require, module)
		module:Destroy()
		if success and type(result) == "table" then
			return result
		else
			warn("Error:", result)
			return nil
		end
	else
		PopUp("Please give the plugin permission to create script.")
	end
end
local function brick()
	warn("Don't touch the core!!")
	PopUp("Modified core, plugin bricked, restart plugin.")
	badword = nil
	k = nil
	pcall(function()
		local ReallyStop = Instance.new("ModuleScript")
		ReallyStop.Name = "Read me..."
		ReallyStop.Source = "--Can you PLEASE don't touche the plugin core ?"
		ReallyStop.Parent = workspace
	end)
	Here.Position = UDim2.new(0.803243995,0,-0.1,0)
	Anti_Virus_Menu:Destroy()
	local Troll_1 = Instance.new("ScreenGui",game.CoreGui)
	Troll_1.Name = "DontChangeAntiVirusCore"
	hereyougobreaker = Troll_1
	local Troll_2 = Instance.new("ImageLabel",Troll_1)
	Troll_2.BackgroundTransparency = 1
	Troll_2.Name = "Asterion"
	Troll_2.Image = "rbxassetid://76985164449462"
	Troll_2.Size = UDim2.new(1,0,1,0)
	local Troll_3 = Instance.new("TextLabel",Troll_2)
	Troll_3.Position = UDim2.new(0,0,0.9,0)
	Troll_3.Size = UDim2.new(1,0,0.1,0)
	Troll_3.Text = "Don't change the Anti-Virus core, you have been warned"
	Troll_3.TextScaled = true
	Troll_3.BackgroundTransparency = 1
	Troll_3.TextColor3 = Color3.fromRGB(255,255,255)
	Troll_3.Font = Enum.Font.Code
	YTextTS("Don't change the Anti-Virus core, you have been warned")
	task.wait(5)
	dobreakcoroutine = true
end
local function ComputePlease(name,defstate)
	local sus,ret = pcall(function()
		if plugin:GetSetting("Data_1")[name] == nil then
			return defstate
		else
			return "yes"
		end
	end)
	if sus then
		if ret == "yes" then
			return plugin:GetSetting("Data_1")[name]
		else
			return defstate
		end
	end
end
local function CreateConfig(name,defstate,details)
	if settingsss[name] == nil then
		settingsss[name] = defstate
	end
	local SettingsConfig_1 = Instance.new("Frame")
	local WhatTheSigma_1 = Instance.new("UIGradient")
	local SettingConfigName_1 = Instance.new("TextLabel")
	local TogglerButton1 = Instance.new("TextButton")
	local ThisCorner2 = Instance.new("UICorner")
	SettingsConfig_1.Name = "SettingsConfig_1"
	SettingsConfig_1.Parent = SettingsScroll
	SettingsConfig_1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	SettingsConfig_1.BorderColor3 = Color3.fromRGB(0, 0, 0)
	SettingsConfig_1.BorderSizePixel = 0
	SettingsConfig_1.Size = UDim2.new(0, 100, 0, 100)
	WhatTheSigma_1.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(0, 0, 0)), ColorSequenceKeypoint.new(0.53, Color3.fromRGB(135, 135, 135)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(45, 45, 45))}
	WhatTheSigma_1.Name = "WhatTheSigma_1"
	WhatTheSigma_1.Parent = SettingsConfig_1
	SettingConfigName_1.Name = "SettingConfigName_1"
	SettingConfigName_1.Parent = SettingsConfig_1
	SettingConfigName_1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	SettingConfigName_1.BackgroundTransparency = 1.000
	SettingConfigName_1.BorderColor3 = Color3.fromRGB(0, 0, 0)
	SettingConfigName_1.BorderSizePixel = 0
	SettingConfigName_1.Size = UDim2.new(0.879924238, 0, 1, 0)
	SettingConfigName_1.Font = Enum.Font.SourceSans
	SettingConfigName_1.Text = name .. " " .. details
	SettingConfigName_1.TextColor3 = Color3.fromRGB(255, 255, 255)
	SettingConfigName_1.TextScaled = true
	SettingConfigName_1.TextSize = 14.000
	SettingConfigName_1.TextWrapped = true
	SettingConfigName_1.TextXAlignment = Enum.TextXAlignment.Left
	TogglerButton1.Name = "TogglerButton1"
	TogglerButton1.Parent = SettingsConfig_1
	TogglerButton1.BackgroundColor3 = ComputePlease(name,defstate) and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 0, 0)
	TogglerButton1.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TogglerButton1.BorderSizePixel = 0
	TogglerButton1.Position = UDim2.new(0.879924238, 0, 0.213333338, 0)
	TogglerButton1.Size = UDim2.new(0.109984763, 0, 0.566666663, 0)
	TogglerButton1.Font = Enum.Font.FredokaOne
	TogglerButton1.Text = ComputePlease(name,defstate) and "On" or "Off"
	TogglerButton1.TextColor3 = Color3.fromRGB(255, 255, 255)
	TogglerButton1.TextScaled = true
	TogglerButton1.TextSize = 14.000
	TogglerButton1.TextWrapped = true
	ThisCorner2.CornerRadius = UDim.new(1, 0)
	ThisCorner2.Name = "ThisCorner2"
	ThisCorner2.Parent = TogglerButton1
	if type(plugin:GetSetting("Data_1")) ~= "table" then
		plugin:SetSetting("Data_1",{[name] = defstate})
	end
	if plugin:GetSetting("Data_1")[name] == true then
		TogglerButton1.BackgroundColor3 = Color3.fromRGB(0,255,0)
		TogglerButton1.Text = "On"
	end
	TogglerButton1.MouseButton1Click:Connect(function()
		if activate then
			settingsss[name] = not settingsss[name]
			if TogglerButton1.Text == "Off" then
				TogglerButton1.BackgroundColor3 = Color3.fromRGB(0,255,0)
				TogglerButton1.Text = "On"
			else
				TogglerButton1.BackgroundColor3 = Color3.fromRGB(255,0,0)
				TogglerButton1.Text = "Off"
			end
			UpdateSettings()
		else
			brick()
		end
	end)
	SettingsScroll.CanvasSize = UDim2.new(0,0,0,UIGridLayout.AbsoluteContentSize.Y)
end
