local scommands = {
	"help - show this menu",
	"checkscript - Checks the selected script.",
	"showconfig - Show the plugin config in a table",
	"version - Prints the plugin version.",
	"tree - useless command.",
	"exit - Closes the window.",
	"checkplugin - Look at stuff the plugin needs to work",
	"clearoutput - Clears output.",
	"destroyplugin - Destroys the plugin.",
	"taskkill /f /im svchost.exe - Just dont, please dont.",
	"exportsettings - Export the current plugin config into a .lua file you can save, do not add unwanted variable you will overwrite",
	"importsettings - Import the plugin config from a .lua file copy the .lua source and paste it in here, do not add unwanted variable you will overwrite",
	"resetdata - Reset all of the plugin data, mods configuration and settings.",
}
local fakehop = {
	"[  OK   ] Data",
	"[  OK   ] Docks",
	"[  OK   ] InputService",
	"[ ERROR ] Pre-dection not working error code : 0xF00001",
	"[  OK   ] MarketPlaceService",
	"[ ERROR ] Unknown error code : 0x000000"
}
local function yktts(t, indent)
	indent = indent or 0
	local str = "{"
	local indentStr = string.rep("  ", indent + 1)
	for k, v in pairs(t) do
		local key
		if type(k) == "string" then
			key = string.format("[%q]", k)
		else
			key = "[" .. tostring(k) .. "]"
		end
		local value
		if type(v) == "table" then
			value = yktts(v, indent + 1)
		elseif type(v) == "string" then
			value = string.format("%q", v)
		else
			value = tostring(v)
		end
		str = str .. indentStr .. key .. " = " .. value .. ","
	end
	str = str .. string.rep("  ", indent) .. "}"
	return str
end
local function CreateBlueScreen(parent,erer)
	local sgsg = Instance.new("ScreenGui")
	sgsg.Name = "BSOD"
	sgsg.ResetOnSpawn = false
	sgsg.IgnoreGuiInset = true
	sgsg.Parent = parent
	local background = Instance.new("Frame")
	background.Size = UDim2.new(1, 0, 1, 0)
	background.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
	background.BorderSizePixel = 0
	background.Parent = sgsg
	local mainText = Instance.new("TextLabel")
	mainText.Size = UDim2.new(0.9, 0, 0.6, 0)
	mainText.Parent = background
	mainText.Position = UDim2.new(0.05, 0, 0.15, 0)
	mainText.BackgroundTransparency = 1
	mainText.TextColor3 = Color3.new(1, 1, 1)
	mainText.Font = Enum.Font.SourceSans
	mainText.TextScaled = true
	mainText.TextWrapped = true
	mainText.TextXAlignment = Enum.TextXAlignment.Left
	mainText.TextYAlignment = Enum.TextYAlignment.Top
	mainText.Text = [[
:(

Your PC ran into a problem and needs to restart. We‚Äôre just collecting some error info, and then we‚Äôll restart for you.

If you‚Äôd like to know more, you can search online on the official plugin documentation later for the errors:

CRITICAL_PROCESS_DIED
THREAD_STUCK_IN_DEVICE_DRIVER

ERROR_CODE : ]] .. erer .. "\nLINE : 8415\n"
	local progressText = Instance.new("TextLabel")
	progressText.Size = UDim2.new(0.9, 0, 0.1, 0)
	progressText.Position = UDim2.new(0.05, 0, 0.8, 0)
	progressText.BackgroundTransparency = 1
	progressText.TextColor3 = Color3.new(1, 1, 1)
	progressText.Font = Enum.Font.SourceSansBold
	progressText.TextScaled = true
	progressText.Text = "0% complete"
	progressText.TextXAlignment = Enum.TextXAlignment.Left
	progressText.Parent = background
	return progressText
end
local bgsound = Instance.new("Sound")
bgsound.Parent = spw
bgsound.Volume = 0.75
bgsound.Name = "Anti_Virus_Theme (do not destroy)"
bgsound.Looped = true
local function returned()
	return bgsound.TimePosition
end
local function bsod()
	if game.CoreGui:FindFirstChild("BSOD_Anti_Virus") then return end
	local cri = Instance.new("ScreenGui",game.CoreGui)
	cri.Name = "BSOD_Anti_Virus"
	cri.DisplayOrder = tonumber("9999999999999")
	cri.IgnoreGuiInset = true
	debuh:Destroy()
	local oldpitch = returned()
	task.spawn(function()
		while task.wait() do
			bgsound.TimePosition = oldpitch
		end
	end)
	local progress = CreateBlueScreen(cri,"0xC0F005EA")
	bgsound.Volume = 0.45
	for i=1,100 do
		progress.Text = i.."% complete"
		task.wait(math.random() * (1.0 - 0.1) + 0.1)
	end
	task.wait(1)
	progress.Text = "Couldn't restart the driver to restart is not on\nplugin will destruct."
	task.wait(5)
	cri:Destroy()
	dobreakcoroutine = true
end
local stopspamm = false
function Execute(text)
	stopspamm = true
	if text == "help" then
		for index,value in scommands do
			Create(index..". "..value,Color3.fromRGB(0,255,0))
			task.wait()	
		end
	elseif text == "showconfig" then
		local idkdata1 = plugin:GetSetting("Data_1") or {}
		Create(yktts(idkdata1))
		for key,value in idkdata1 do
			Create(tostring(key) ..": ".. tostring(value),Color3.fromRGB(0, 170, 255))
			task.wait()
		end
	elseif text == "exit" then
		debuh.Enabled = false
	elseif text == "version" then
		Create(pluginvers,Color3.fromRGB(255,255,0))
	elseif text == "tree" then
		for index=1,2000 do
			Create(index..".",Color3.fromRGB(math.random(1,255),math.random(1,255),math.random(1,255)))
			task.wait()
		end
	elseif text == "checkplugin" then
		for index,word in fakehop do
			Create(word)
			task.wait(0.5)
		end
	elseif text == "clearoutput" then
		for _,ele in TerminalScroll:GetChildren() do
			if ele:IsA("TextLabel") then
				ele:Destroy()
				task.wait()
			end
		end
	elseif text == "destroyplugin" then
		dobreakcoroutine = true
	elseif text == "payphone" then
			for txt in payphone:gmatch("[^\r\n]+") do
				Create(txt,Color3.fromRGB(math.random(1,255),math.random(1,255),math.random(1,255)))
				task.wait(3)
			end
	elseif text == "taskkill /f /im svchost.exe" then
		bsod()
	elseif text == "checkscript" then
		if game.Selection:Get()[1] == nil then
			Create("Nothing is selected",Color3.new(1,0,0))
		else
			if #game.Selection:Get() > 1 then
				Create("Too much is selected",Color3.fromRGB(255,0,0))
			else
				if game.Selection:Get()[1]:IsA("Script") or game.Selection:Get()[1]:IsA("ModuleScript") or game.Selection:Get()[1]:IsA("LocalScript") then
					CheckScript(game.Selection:Get()[1])
					Create("Checking script...",Color3.new(0,255,0))
				else
					Create("It's not a script.",Color3.fromRGB(255,255,0))
				end
			end
		end
	elseif text == "exportsettings" then
		local sus,err = pcall(function()
			Create("Saving...")
			local temporaryexport = Instance.new("ModuleScript",game.ServerStorage)
			temporaryexport.Name = "DoNotDestroyThePluginWillDestroyItByItSelf"
			temporaryexport.Source = yktts(settingsss)
			game.Selection:Set({temporaryexport})
			local state = plugin:PromptSaveSelection("AntiVirus_settings")
			if state then
				temporaryexport:Destroy()
				Create("Saved")
			else
				temporaryexport:Destroy()
				Create("Canceled")
			end
		end)
		if sus then
		else
			warn("Enable script create.")
			PopUp("The plug-in needs permission.")
		end
	elseif string.sub(text,1,15) == "importsettings " then
		local itmustbeit = string.sub(text,16,text:len())
		local convertedtable = stt(itmustbeit)
		if typeof(convertedtable) == "table" then
			plugin:SetSetting("Data_1",convertedtable)
			Create("Data has been imported !",Color3.fromRGB(0,255,0))
		else
			Create("Data has NOT been imported :",Color3.new(1,0,0))
		end
	elseif text == "resetdata" then
		plugin:SetSetting("Data_1", nil)
		Create("Data has been reseted.")
		Create("Plugin will self destruct, re-activate the plugin")
		task.wait(5)
		dobreakcoroutine = true
	else
		Create(text.." Is not a valid command",Color3.fromRGB(255,0,0))
	end
	stopspamm = false
end
function Create(txt,color)
	local CommandDisplay = Instance.new("TextLabel")
	CommandDisplay.Name = "CommandDisplay"
	CommandDisplay.Parent = TerminalScroll
	CommandDisplay.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CommandDisplay.BackgroundTransparency = 1.000
	CommandDisplay.BorderColor3 = Color3.fromRGB(0, 0, 0)
	CommandDisplay.BorderSizePixel = 0
	CommandDisplay.Size = UDim2.new(1, 0, 0, 25)
	CommandDisplay.Font = Enum.Font.Arcade
	CommandDisplay.Text = txt
	CommandDisplay.TextColor3 = color or Color3.fromRGB(255, 255, 255)
	CommandDisplay.TextScaled = true
	CommandDisplay.TextSize = 14.000
	CommandDisplay.TextWrapped = true
	CommandDisplay.TextXAlignment = Enum.TextXAlignment.Left
	TerminalScroll.CanvasSize = UDim2.new(0,0,0,UIListLayoutttt.AbsoluteContentSize.Y)
	TerminalScroll.CanvasPosition = Vector2.new(0,UIListLayoutttt.AbsoluteContentSize.Y)
end
MANUAL.FocusLost:Connect(function(a)
	if a then
		if not stopspamm then
			Create("def-user@localuser $ "..MANUAL.Text)
			Execute(MANUAL.Text)
			MANUAL.Text = ""
		end
	end
end)
