function ErrorSSound()
	task.spawn(function()
		local sous = Instance.new("Sound",spw)
		sous.SoundId = "rbxassetid://2865228021"
		sous.Volume = 1
		sous:Play()
		sous.Ended:Wait()
		sous:Destroy()
	end)
end
