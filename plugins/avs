task.spawn(function()
	while task.wait(60) do
		if dobreakcoroutine then
			break
		end
		local letsee = getvers()
		if letsee == pluginvers then
			
		else
			if letsee == false then
				bsod()
			else
				if settingsss["Warned when new update"] == true then
					PopUp("Your plugin version is outdated please use new version.")
				end
			end
		end
	end
end)
task.spawn(function()
	while task.wait(20) do
		if dobreakcoroutine then
			break
		end
		if settingsss["Real-time protection"] == true then
			CheckViruses()
		end
	end
end)
task.spawn(function()
	while task.wait(3) do
		if dobreakcoroutine then
			break
		end
		LogScrollingFrame.CanvasSize = UDim2.new(0,0,0,LogUIGridLayout.AbsoluteContentSize.Y)
		CreateCacheFolder()
		for _,txt in pairs(LogScrollingFrame:GetChildren()) do
			if txt:IsA("TextLabel") then
				txt:Destroy()
				LogScrollingFrame.CanvasSize = UDim2.new(0,0,0,LogUIGridLayout.AbsoluteContentSize.Y)
			end
		end
		CreateCacheFolder()
		for _,v in pairs(game.ServerStorage["81396126331815_AntiVirus_Cache"]:GetChildren()) do
			if v:IsA("StringValue") then
				local newcachetxt = Instance.new("TextLabel")
				newcachetxt.Name = "LogTitle"
				newcachetxt.Parent = LogScrollingFrame
				newcachetxt.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				newcachetxt.BackgroundTransparency = 1.000
				newcachetxt.BorderColor3 = Color3.fromRGB(0, 0, 0)
				newcachetxt.BorderSizePixel = 0
				newcachetxt.Position = UDim2.new(-4.93971832e-08, 0, 1.94826217e-08, 0)
				newcachetxt.Size = UDim2.new(1.00000012, 0, 0.146373823, 0)
				newcachetxt.Font = Enum.Font.DenkOne
				newcachetxt.Text = v.Name .. "\n" .. v.Value
				newcachetxt.TextColor3 = Color3.fromRGB(255, 255, 255)
				newcachetxt.TextSize = 25
				newcachetxt.TextXAlignment = Enum.TextXAlignment.Left
				LogScrollingFrame.CanvasSize = UDim2.new(0,0,0,LogUIGridLayout.AbsoluteContentSize.Y)
			end
			task.wait()
		end
	end
end)
task.spawn(function()
	while task.wait(0.5) do
		if settingsss["Background music"] == true then
			PlaySound()
		else
			bgsound:Stop()
		end
	end
end)
task.spawn(function()
	local alreadystopping = false
	while task.wait() do
		if dobreakcoroutine == true then
			if not alreadystopping then
				alreadystopping = true
				Anti_Virus_Menu:Destroy()
				Areyounotsure:Destroy()
				Scan:Destroy()
				PopUpg:Destroy()
				debuh:Destroy()
				spw:Destroy()
				warn("Plugin destructed.")
				script.Disabled = true
				break
			end
		end
	end
end)
