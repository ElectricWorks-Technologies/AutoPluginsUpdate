local void = DockWidgetPluginGuiInfo.new(
	Enum.InitialDockState.Float,
	false,
	false,
	0,
	0,
	0,
	0
)
local function TranslateTextAsync(lang,text)
	local LocalizationService = game:GetService("LocalizationService")
	local success, translator = pcall(function()
		return LocalizationService:GetTranslatorForLocaleAsync(lang)
	end)
	if success then
		local result = translator:Translate(text, "Hello")
		return result
	else
		print("GetTranslatorForLocaleAsync failed: " .. translator)
	end
end
local spw = plugin:CreateDockWidgetPluginGui("SoundPlayerWidget", void)
spw.Enabled = false
spw.Name = "SoundHandler"
local settingsss = plugin:GetSetting("Data_1") or {}
local function UpdateSettings()
	plugin:SetSetting("Data_1",settingsss)
end
local function getvers()
	local y,r = pcall(function()
		return mps:GetProductInfo(81396126331815).Description:match("Version%s*:%s*([%w%.%-]+)")
	end)
	if y then
		return r
	else
		warn("Not loaded")
		return false
	end
end
local cooldown = false
local activate = false
local RunService = game:GetService("RunService")
local versions = getvers()
if pluginvers == versions then
else
	warn("Outdated version of plugin please change it")
end
local OnOff = false
if not RunService:IsRunning() then
	OnOff = true
else
	OnOff = false
end
