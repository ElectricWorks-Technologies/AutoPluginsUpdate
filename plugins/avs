				wait(0.5)
				Activate.Text = "Activate"
			end
			keycoll = false
		end
	end
end)
local function parsePath(pathString)
	local pathParts = {}
	for part in pathString:gmatch("[^%.%[]+") do
		part = part:gsub("[\"'%]]", ""):gsub("^%s+", ""):gsub("%s+$", "")
		if part ~= "" then
			table.insert(pathParts, part)
		end
	end
	return pathParts
end
local function followPath(startObj, pathTable)
	local current = startObj
	for i = 2, #pathTable do
		if typeof(current) == "Instance" and current:FindFirstChild(pathTable[i]) then
			current = current[pathTable[i]]
		else
			return nil
		end
	end
	return current
end
function CheckScript(sc)
	if activate then
		if not dobreakcoroutine then
			Create("Checking script :"..sc.Name.. " at :".. sc:GetFullName())
			task.spawn(function()
				for _, word in ipairs(badword) do
					if sc.Source:find(word) then
						CreateError(sc, Output)
					end
					task.wait()
				end
				for id in sc.Source:gmatch("re".."qu".."ire%s*%((.-)%)") do
					if tonumber(id) then
						if mps:GetProductInfo(id).AssetTypeId == 10 then
							Create("This script requires a module script, Roblox wont allow me to use something to load the required script so I will just flag it as a virus, also please follow me and favorite the plugin, it really helps me get verified")
							CreateError(sc, Output)
						end
					end
					if type(id) == "string" and not tonumber(id) then
						Create("String")
						local s = parsePath(id)
						Create(s)
						local lastin = followPath(sc,s)
						Create(lastin)
						if not (lastin:IsA("NumberValue") or lastin:IsA("IntValue") or lastin:IsA("StringValue")) then
							if tonumber(lastin.Name) then
								if mps:GetProductInfo(lastin.Name).AssetTypeId == 10 then
									CreateError(sc, Output)
								end
							end
						else
							if tonumber(lastin.Value) then
								if mps:GetProductInfo(lastin.Value).AssetTypeId == 10 then
									CreateError(sc, Output)
								end
							end
						end
					end
				end
			end)
		end
	end
end
local function IsAScript(inst:Instance)
	if string.sub(inst:GetFullName(),1,7) == "CoreGui" then
	else
		if inst:IsA("Script") then
			return true
		end
		if inst:IsA("ModuleScript") then
			return true
		end
		if inst:IsA("LocalScript") then
			return true
		end
	end
end
local function CheckViruses()
	if activate then
		if cooldown == false then
			cooldown = true
			Create("Checking viruses.")
			for _,kk in pairs(Output:GetChildren()) do
				if kk:IsA("Frame") then
					kk:Destroy()
				end
			end
			for _,Scr2Chk in pairs(game:GetDescendants()) do
				if IsAScript(Scr2Chk) then
					CheckScript(Scr2Chk)
				end
			end
			cooldown = false
		end
	end
end
game.DescendantAdded:Connect(function(obj)
	if IsAScript(obj) then
		if settingsss["Check script that are added"] == true then
			Create("New script added, checking the script.")
			CheckScript(obj)
		end
	end
end)
ScanButton.MouseButton1Click:Connect(function()
	if activate then
		CheckViruses()
	else
		brick()
	end
end)
ClearButton.MouseButton1Click:Connect(function()
	if activate then
		if cooldown == false then
			for _,k in pairs(Output:GetChildren()) do
				if k:IsA("Frame") then
					k:Destroy()
				end
			end
		end
	end
end)
